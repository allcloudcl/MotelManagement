
    <%#if current_admin.expired_at < Time.now %>
      <%#= note %>
      <%#end %>

    <header id="first">
        <div class="header-content">
          <p id="notice"><%= @notice1 %></p>
        <%if !admin_signed_in? %>
            <div class="inner">
                <h2 class="Helvetica">WELCOME TO MANAGEMENT MOTEL SYSTEM</h2>
                <hr>
                <a href="/guest" id="guest" class="btn btn-primary btn-xl"> GUEST </a> &nbsp; <a href="/admins/sign_in" class="btn btn-primary btn-xl"> MANAGER </a>
            </div>
        <%else %>
        <div class="inner">
                <h2 class="Helvetica">HELLO <%= current_admin.email %></h2>
                <hr> <%= link_to 'MY INFORMATION', edit_admin_registration_path %>
